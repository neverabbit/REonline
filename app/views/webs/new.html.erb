<div class="col-md-6 col-md-offset-3">
<%= form_for(@web) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name, class: "form-control" %>
	
	<%= f.label :address %>
	<%= f.text_field :address, class: "form-control" %>
	
	<%= f.label :image_big_path %>
	<%= f.file_field :image_big_path, class: "form-control" %>
	
	<%= f.label :description %>
	<%= f.text_area :description, class: "form-control" %>
	
	<%= f.submit "Post Webapp", class: "btn btn-primary" %>
<% end %>	
</div>